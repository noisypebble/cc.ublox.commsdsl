            <bundle name="lists" displayName="_">
                <list name="sf1d" count="8">
                    <int name="element" type="uint32" />
                </list>
                <list name="sf2d" count="8">
                    <int name="element" type="uint32" />
                </list>     
                <list name="sf3d" count="8">
                    <int name="element" type="uint32" />
