<?xml version="1.0" encoding="UTF-8"?>
<schema name="ublox">
    <message name="AidIni" id="MsgId.AidIni" displayName="AID-INI" order="0">
        <optional name="ecefX" defaultMode="exist" displayExtModeCtrl="true">
            <int name="ecefX" type="int32" units="cm" />
        </optional>
        <optional name="lat" defaultMode="missing" displayExtModeCtrl="true">        
            <int name="lat" type="int32" units="deg" scaling="1/10000000" displayDecimals="7"/>
        </optional>
        <optional name="ecefY" defaultMode="exist" displayExtModeCtrl="true">
            <int name="ecefY" type="int32" units="cm" />
        </optional>
        <optional name="lon" defaultMode="missing" displayExtModeCtrl="true">        
            <int name="lon" type="int32" units="deg" scaling="1/10000000" displayDecimals="7"/>
        </optional>
        <optional name="ecefZ" defaultMode="exist" displayExtModeCtrl="true">
            <int name="ecefZ" type="int32" units="cm" />
        </optional>
        <optional name="alt" defaultMode="missing" displayExtModeCtrl="true">        
            <int name="alt" type="int32" units="cm"/>
        </optional>
        <int name="posAcc" type="uint32" units="cm" />
        <set name="tmCfg" length="2">
            <bit name="fEdge" idx="1" />
            <bit name="tm1" idx="4" />
            <bit name="f1" idx="6" />
        </set>
